<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:p="http://www.springframework.org/schema/p"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="aobj" class="com.mncindia.spring.A" p:a="10" p:msg="Hi Pk ,how r u?"/>


<bean id="bobj" class="com.mncindia.spring.B" p:b="100" p:msg="Hello Sk ,i am fine" />

<bean id="hello" class="com.mncindia.spring.Hello" p:aobj-ref="aobj" p:bobj-ref="bobj"/>

</beans>